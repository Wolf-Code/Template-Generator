{
	"name": "templategenerator",
	"displayName": "Template Generator",
	"description": "Generates your files and folders based on your own template, including custom parameters!",
	"version": "0.2.0",
	"publisher": "kevin-deelen",
	"repository": {
		"url": "https://github.com/Wolf-Code/Template-Generator"
	},
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Snippets",
		"Other"
	],
	"activationEvents": [
		"onCommand:templateGenerator.generateTemplate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "templateGenerator.generateTemplate",
				"title": "Generate template"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "templateGenerator.generateTemplate"
				}
			]
		},
		"configuration": [
			{
				"title": "Templates",
				"properties": {
					"templates.path": {
						"type": "string",
						"description": "The path to the directory containing all templates"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/lodash": "^4.14.160",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"lodash": "^4.17.20"
	}
}
